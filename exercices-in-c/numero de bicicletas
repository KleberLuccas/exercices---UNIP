#include<stdio.h>

int main()
{
    int aparecida = 1500, tokio = 55000, anos = 0;

    for (;;)
    {
        anos = anos+1;
        aparecida = aparecida * 0.89;
        tokio = tokio * 0.05;

        if (aparecida == tokio)
        {
            break;
        }
    }

    printf("Levou apenas %d anos", anos);
}
