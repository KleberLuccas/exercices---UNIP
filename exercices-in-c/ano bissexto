#include<stdio.h>

int main()
{
    int ano;

    printf("Digite o ano\n");
    scanf("%d", &ano);

    if (ano <10000)
    {
    if (ano % 100 == 0)
    {
        if (ano % 400 == 0)
        {
            printf ("O ano e bissexto");
        }
        else
        {
            printf("O ano nao e bissexto");
        }
    }
    else if (ano % 4 == 0)
    {
        printf("O ano e bissexto");
    }
    else
        printf("O ano nao e bissexto");
    }
    else
        printf("O numero tem 5 digitos");
}
