#include<stdio.h>

int main(void){
    float celsius, fahrenheit, kelvin


    printf("Insira a temperatura em Celsius");
    scanf("%f", &celsius);

    kelvin = Celsius + 273;

    fahrenheit = (9*celsius)/5 + 32;
    
    printf("a temperatura em Kelvi é %f e em Fahrenheit é %f", kelvin, fahrenheit);
    
    return 0;

}
